<div class="container">
  <h1>Create a new list</h1>

  <%= simple_form_for (@list) do |f| %>
    <div class="form-inputs">
        <%= f.input :name %>
        <%= f.input :poster_url %>
    </div>

    <div class="form-actions">
      <%= f.button :submit, class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
